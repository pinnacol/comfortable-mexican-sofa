<%= form.text_field :name %>
<% if @cms_seed.new_record? %>
  <% if @cms_seed.exporting? %>
    <%= hidden_field_tag(:export, 1) %>
    <%= form.select :site_id, options_from_collection_for_select(CmsSite.all, "id", "label"), :include_blank => true %>
  <% else %>
    <%= form.file_field :zip_file %>
  <% end %>
<% end %>